{
  "name": "@finografic/core",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.8.0",
  "description": "finografic core package - types, utils, constants, etc.",
  "author": {
    "name": "Justin",
    "email": "justin.blair.rankin@gmail.com",
    "url": "http://finografic.github.com/cv-justin-rankin"
  },
  "license": "MIT",
  "keywords": [
    "finografic",
    "core",
    "types",
    "utils",
    "constants",
    "api"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./api": {
      "types": "./dist/api/index.d.ts",
      "import": "./dist/api/index.js"
    },
    "./api/*": {
      "types": "./dist/api/*.d.ts",
      "import": "./dist/api/*.js"
    },
    "./constants": {
      "types": "./dist/constants/index.d.ts",
      "import": "./dist/constants/index.js"
    },
    "./constants/*": {
      "types": "./dist/constants/*.d.ts",
      "import": "./dist/constants/*.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    },
    "./types/*": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/types/*.js"
    },
    "./types/utils": {
      "types": "./dist/types/utils/index.d.ts",
      "import": "./dist/types/utils/index.js"
    },
    "./types/utils/*": {
      "types": "./dist/types/utils/*.d.ts",
      "import": "./dist/types/utils/*.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    },
    "./utils/*": {
      "types": "./dist/utils/*.d.ts",
      "import": "./dist/utils/*.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "========== DEV": "=======================================================",
    "dev": "tsup --watch",
    "·········· BUILD": "·····················································",
    "build": "tsup",
    "·········· LINTING": "···················································",
    "lint": "eslint .",
    "lint.fix": "eslint . --fix",
    "·········· UTILS": "·····················································",
    "tsc.debug": "pnpm exec tsc --pretty --project tsconfig.json",
    "typecheck": "pnpm exec tsc --project tsconfig.json --noEmit",
    "·········· HOOKS": "·····················································",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "axios": "1.8.4",
    "lodash": "4.17.21",
    "stoker": "^1.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/lodash": "4.17.13",
    "@types/node": "20.15.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "9.16.0",
    "lint-staged": "^16.1.2",
    "simple-git-hooks": "^2.13.0",
    "tsup": "8.4.0",
    "typescript": "^5.8.3",
    "zod": "^3.21.4"
  },
  "peerDependencies": {
    "typescript": "^5.8.3",
    "zod": "^3.21.4"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs}": [
      "eslint --fix"
    ]
  }
}
